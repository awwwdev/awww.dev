{
  "name": "awww.dev",
  "description": "Hamid K. Portfolio Website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "format": "prettier --write .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "test": "npm run check-lint && npm run check-types",
    "dev-turbo": "concurrently \"pnpm unocss:dev\"  \"next dev --turbo\"",
    "dev": "concurrently \"pnpm unocss:dev\"  \"next dev\"",
    "build": "pnpm unocss:build && next build",
    "start": "next start",
    "lint": "next lint",
    "unocss:dev": "unocss -c uno.config.ts -o styles/uno.css --watch  \"./**/*.tsx\"  \"./**/*.md\" \"./lib/api.ts\" ",
    "unocss:build": "unocss -c uno.config.ts -o styles/uno.css  \"./**/*.tsx\"  \"./**/*.md\" \"./lib/api.ts\" "
  },
  "dependencies": {
    "@iconify-json/logos": "^1.1.43",
    "@leafac/rehype-shiki": "^2.2.1",
    "@radix-ui/colors": "^3.0.0",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toggle": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.2",
    "@unocss/cli": "^0.58.5",
    "@unocss/rule-utils": "^0.62.3",
    "colorjs.io": "^0.5.2",
    "framer-motion": "^11.3.24",
    "gray-matter": "^4.0.3",
    "next": "^14.2.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rehype-add-classes": "^1.0.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-pretty-code": "^0.13.2",
    "rehype-raw": "^7.0.0",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.0",
    "rehype-video": "^2.2.0",
    "remark-gfm": "^4.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.0",
    "shiki": "^1.10.3",
    "unified": "^11.0.5",
    "unocss": "^0.62.3",
    "unocss-preset-theme": "^0.13.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@iconify-json/bxl": "^1.1.10",
    "@iconify-json/iconoir": "^1.1.44",
    "@iconify-json/ph": "^1.1.13",
    "@iconify-json/svg-spinners": "^1.1.2",
    "@svgr/webpack": "^6.5.1",
    "@types/node": "18.8.5",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@unocss/reset": "^0.47.4",
    "@unocss/webpack": "^0.47.4",
    "concurrently": "^8.2.2",
    "eslint": "8.25.0",
    "eslint-config-next": "13.0.5",
    "eslint-config-prettier": "^8.6.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.3",
    "sass": "^1.56.1",
    "typescript": "4.8.4",
    "unocss-preset-primitives": "0.0.1-beta.5",
    "unocss-preset-radix": "^2.5.5"
  }
}